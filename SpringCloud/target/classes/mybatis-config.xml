  "http://mybatis.org/dtd/mybatis-3-config.dtd">
  
 <configuration>
 	
 	<typeAliases>
 		<!-- Resource make aliases -->
 		<typeAlias type="com.iof.spring.user.model.VO.UserVO" alias ="user"/>
 		<typeAlias type="com.iof.spring.site.model.VO.SiteVO" alias="site"/>
 		<typeAlias type="com.iof.spring.plot.model.VO.PlotVO" alias="plot"/>
 		<typeAlias type="com.iof.spring.device.model.VO.DeviceVO" alias="device"/>
