 		<typeAlias type="com.iof.spring.user.model.VO.JoinUserVO" alias ="joinUser"/>
 		<typeAlias type="com.iof.spring.site.model.VO.JoinSiteVO" alias="joinSite"/>
 		<typeAlias type="com.iof.spring.plot.model.VO.JoinPlotVO" alias="joinPlot"/>
